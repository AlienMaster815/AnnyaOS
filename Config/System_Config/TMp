        <FILETOCPY64>cp DLL/LouDLLs/NtDll/ntdll.dll ISO/ANNYA/SYSTEM64/NTDLL.DLL</FILETOCPY64>                              <!--Nt Kernel User Space Abstraction Layer-->
        <FILETOCPY64>cp DLL/LouDLLs/kernelbase/kernbase.dll ISO/ANNYA/SYSTEM64/KERNBASE.DLL</FILETOCPY64>                   <!--Windows Legacy(9x 16 bit Kernel) Abstraction Layer-->


echo 'module_path: boot():/ANNYA/SYSTEM64/CONFIG/BOOT.CFG' >> ISO/boot/limine.conf ;
echo 'module_path: boot():/ANNYA/SYSTEM64/DRIVERS/AHCI.SYS' >> ISO/boot/limine.conf ;
echo 'module_path: boot():/ANNYA/SYSTEM64/DRIVERS/PIIX.SYS' >> ISO/boot/limine.conf ;
echo 'module_path: boot():/ANNYA/SYSTEM64/DRIVERS/XHCI.SYS' >> ISO/boot/limine.conf ;

        <FILETOCPY64>cp Config/System_Config/DriverManifest.cfg ISO/ANNYA/SYSTEM64/CONFIG/DRIVERS.CFG</FILETOCPY64>        <!--Annya OS Driver Manifest Registry-->
        <FILETOCPY64>cp Config/System_Config/BootManifest.cfg ISO/ANNYA/SYSTEM64/CONFIG/BOOT.CFG</FILETOCPY64>                     <!--Annya OS BOOT Manifest Registry-->
echo 'module_path: boot():/ANNYA/SYSTEM64/DRIVERS/PIIX.SYS' >> ISO/boot/limine.conf ;
